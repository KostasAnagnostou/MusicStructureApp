<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructureapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="17"
        android:targetSdkVersion="25" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_pink"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.example.android.musicstructureapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.musicstructureapp.ArtistsActivity"
            android:label="@string/artists" />
        <activity
            android:name="com.example.android.musicstructureapp.SongsActivity"
            android:label="@string/songs" />
        <activity
            android:name="com.example.android.musicstructureapp.TopTenActivity"
            android:label="@string/top_ten" />
        <activity
            android:name="com.example.android.musicstructureapp.MyPlaylist"
            android:label="@string/playlist" />
        <activity
            android:name="com.example.android.musicstructureapp.ListenSongActivity"
            android:label="@string/listen_song" />
    </application>

</manifest>