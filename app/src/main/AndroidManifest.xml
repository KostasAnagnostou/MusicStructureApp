<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo_pink"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ArtistsActivity"
            android:label="@string/artists" />
        <activity
            android:name=".SongsActivity"
            android:label="@string/songs" />
        <activity
            android:name=".TopTenActivity"
            android:label="@string/top_ten" />
        <activity
            android:name=".MyPlaylist"
            android:label="@string/playlist" />
        <activity
            android:name=".ListenSongActivity"
            android:label="@string/listen_song" />

    </application>

</manifest>